<!DOCTYPE html>
<html>
  <head>
    <title><%= name %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://static.robotwebtools.org/roslibjs/current/roslib.min.js"></script>
  </head>
  <body>

    <% include partial %>

    <h2><%= name %></h2>

    <div>
        
      <form id="form">
        <div class="monitor">
          <input id="waypoint_id" name="waypoint_id" type="text">
        </div>

        <br>
        
        <div class="monitor">
          <input type='button' value='Add Waypoint' onclick="add_waypoint()">
        </div>

        <br>
        
        <div class="monitor">
          <input type='button' value='Delete Waypoint' onclick="delete_waypoint()">
        </div>

        <br>

        <div class="monitor">
          <input type='button' value='Save Waypoints' onclick="save_waypoints()">
        </div>

      </form>
      
    </div>
  </body>
  <script src="/js/waypoint.js"></script>
</html>
