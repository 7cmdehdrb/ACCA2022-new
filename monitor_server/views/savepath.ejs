<!DOCTYPE html>
<html>
  <head>
    <title><%= name %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://static.robotwebtools.org/roslibjs/current/roslib.min.js"></script>
  </head>
  <body>

    <% include partial %>

    <h2><%= name %></h2>

    <div>
        
      <form id="form">
        <div class="monitor">
          <input id="start" name="start" type="text" placeholder="Start">
        </div>

        <br>
        
        <div class="monitor">
          <input id="end" name="end" type="text" placeholder="End">
        </div>

        <br>

        <div class="monitor">
          <input type='button' value='Save' onclick="savePath()">
        </div>

      </form>
      
    </div>
  </body>
  <script src="/js/savepath.js"></script>
</html>
